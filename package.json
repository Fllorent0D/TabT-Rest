{
  "name": "tabt-rest",
  "version": "1.20.89",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "engines": {
    "node": "20.12.x",
    "npm": "10.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "ci": "node ./node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test-e2e/**/*.ts\\\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config test-e2e/jest-e2e.json",
    "release": "release-it",
    "start:azure": "npm run build && func host start",
    "prisma:deploy": "prisma migrate deploy",
    "start:migrate:prod": "npm run prisma:deploy && npm run start:prod"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.2",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "10.3.7",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.7",
    "@nestjs/platform-express": "10.3.7",
    "@nestjs/schedule": "4.0.2",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "7.3.1",
    "@nestjs/terminus": "10.2.3",
    "@prisma/client": "5.12.1",
    "@xmldom/xmldom": "0.8.10",
    "bluebird": "3.7.2",
    "cache-manager": "5.5.1",
    "cache-manager-memory-store": "1.1.0",
    "cache-manager-redis-store": "3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "compression": "1.7.4",
    "date-fns": "3.6.0",
    "form-data": "4.0.0",
    "he": "1.2.0",
    "helmet": "7.1.0",
    "jsdom": "24.0.0",
    "lodash": "4.17.21",
    "lunr": "2.3.9",
    "nestjs-pino": "4.0.0",
    "p-queue": "8.0.1",
