{
  "name": "tabt-rest",
  "version": "1.4.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "ci": "node ./node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test-e2e/**/*.ts\\\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges output ./badges",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config test-e2e/jest-e2e.json",
    "jest-coverage-badges": "jest-coverage-badges output ./badges"
  },
  "dependencies": {
    "@logtail/node": "^0.1.7",
    "@nestjs/common": "^7.6.11",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.6.11",
    "@nestjs/platform-express": "^7.6.11",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/terminus": "^7.1.0",
    "bluebird": "^3.7.2",
    "cache-manager": "^3.4.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "dd-trace": "^1.3.0",
    "flatted": "3.1.0",
    "form-data": "^3.0.0",
    "hbs": "^4.1.1",
    "helmet": "^4.1.1",
    "hot-shots": "^8.5.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "nest-morgan": "^1.0.1",
    "nestjs-pino": "^1.2.0",
    "nominatim-geocoder": "^0.1.4",
    "pino-pretty": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "^6.5.4",
    "soap": "^0.36.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.1",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/compression": "^1.7.0",
    "@types/continuation-local-storage": "^3.2.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.13.25",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.3.0",
    "jest": "26.4.2",
    "jest-coverage-badges": "^1.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      "entity",
      ".contract.ts",
      ".constants.ts",
      ".decorator.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  }
}
