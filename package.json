{
  "name": "tabt-rest",
  "version": "1.9.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "engines" : {
    "node" : "16.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "ci": "node ./node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test-e2e/**/*.ts\\\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges output ./badges",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config test-e2e/jest-e2e.json",
    "jest-coverage-badges": "jest-coverage-badges output ./badges",
    "gcp-build": "npm run build"
  },
  "dependencies": {
    "@elastic/elasticsearch": "8.1.0",
    "@logtail/node": "0.1.10",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.4",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.4.4",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/schedule": "1.1.0",
    "@nestjs/serve-static": "2.2.2",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/terminus": "8.0.6",
    "@types/lunr": "2.3.4",
    "bluebird": "3.7.2",
    "cache-manager": "3.6.1",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "flatted": "3.2.5",
    "form-data": "4.0.0",
    "hbs": "4.2.0",
    "he": "1.2.0",
    "helmet": "5.0.2",
    "lodash": "4.17.21",
    "lunr": "2.3.9",
    "morgan": "1.10.0",
    "nestjs-pino": "2.5.2",
    "nominatim-geocoder": "0.1.4",
    "pino-http": "6.6.0",
    "pino-multi-stream": "6.0.0",
    "pino-pretty": "7.6.1",
    "reflect-metadata": "0.1.13",
    "response-time": "2.3.2",
    "rxjs": "7.5.5",
    "soap": "0.43.0",
    "swagger-ui-express": "4.3.0",
    "uuid": "8.3.2",
    "xmldom": "0.6.0"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "3.3.0",
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.10",
    "@nestjs/testing": "8.4.4",
    "@types/bluebird": "3.5.36",
    "@types/compression": "1.7.2",
    "@types/continuation-local-storage": "3.2.4",
    "@types/cron": "1.7.3",
    "@types/express": "4.17.13",
    "@types/he": "1.1.2",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.32",
    "@types/response-time": "2.3.5",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "7.0.4",
    "jest": "28.0.2",
    "jest-coverage-badges": "1.1.2",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "supertest": "6.2.3",
    "ts-jest": "28.0.0-next.1",
    "ts-loader": "9.2.9",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      "entity",
      ".contract.ts",
      ".constants.ts",
      ".decorator.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  }
}
