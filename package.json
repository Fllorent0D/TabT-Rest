{
  "name": "tabt-rest",
  "version": "1.7.4",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "ci": "node ./node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test-e2e/**/*.ts\\\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges output ./badges",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config test-e2e/jest-e2e.json",
    "jest-coverage-badges": "jest-coverage-badges output ./badges"
  },
  "dependencies": {
    "@logtail/node": "^0.1.7",
    "@nestjs/axios": "^0.0.2",
    "@nestjs/common": "^8.0.9",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.9",
    "@nestjs/platform-express": "^8.0.9",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/terminus": "^8.0.1",
    "bluebird": "^3.7.2",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "dd-trace": "^1.3.0",
    "flatted": "3.1.0",
    "form-data": "^3.0.1",
    "hbs": "^4.1.1",
    "he": "^1.2.0",
    "helmet": "^4.6.0",
    "hot-shots": "^8.5.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "nest-morgan": "^2.0.0",
    "nestjs-pino": "^1.2.0",
    "nominatim-geocoder": "^0.1.4",
    "pino-pretty": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "^7.3.1",
    "soap": "^0.36.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.1",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.1.1",
    "@types/bluebird": "^3.5.32",
    "@types/compression": "^1.7.2",
    "@types/continuation-local-storage": "^3.2.3",
    "@types/express": "^4.17.13",
    "@types/he": "^1.1.2",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.10.2",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^4.3.0",
    "jest": "^27.2.4",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "10.3.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      "entity",
      ".contract.ts",
      ".constants.ts",
      ".decorator.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  }
}
